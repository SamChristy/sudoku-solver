  "scripts": {
    "unpack-scripts": "rsync --checksum --ignore-times node_modules/opencv4js/opencv.js public/lib && rsync node_modules/tesseract.js/dist/worker.min.js public/lib/ocr && rsync node_modules/tesseract.js-core/tesseract-core.wasm.js public/lib/ocr",
    "start": "npm run unpack-scripts && HTTPS=true react-scripts start",
    "build": "react-scripts build && npm run unpack-scripts",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color --fix",
